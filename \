
# Setting PATH for Python 3.8
# The original version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/3.8/bin:${PATH}"
export PATH

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/mohammed/google-cloud-sdk/path.bash.inc' ]; then . '/Users/mohammed/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/mohammed/google-cloud-sdk/completion.bash.inc' ]; then . '/Users/mohammed/google-cloud-sdk/completion.bash.inc'; fi
# added by Anaconda3 2019.10 installer
# >>> conda init >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$(CONDA_REPORT_ERRORS=false '/Users/mohammed/opt/anaconda3/bin/conda' shell.bash hook 2> /dev/null)"
if [ $? -eq 0 ]; then
    \eval "$__conda_setup"
else
    if [ -f "/Users/mohammed/opt/anaconda3/etc/profile.d/conda.sh" ]; then
# . "/Users/mohammed/opt/anaconda3/etc/profile.d/conda.sh"  # commented out by conda initialize
        CONDA_CHANGEPS1=false conda activate base
    else
        \export PATH="/Users/mohammed/opt/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda init <<<

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/mohammed/opt/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/mohammed/opt/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/mohammed/opt/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/mohammed/opt/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<
export SDX_HOME=/Users/mohammed/projects/sdx

function spinnaker() {
gcloud container clusters get-credentials k8s-management --region europe-west2 --project ons-rasrmbs-management
kubectl config set-context gke_ons-rasrmbs-management_europe-west2_k8s-management --namespace spinnaker
kubectl config use-context gke_ons-rasrmbs-management_europe-west2_k8s-management
}

function khanm1sand() {
gcloud container clusters get-credentials k8s-ras --region europe-west2 --project rm-ras-sandbox
kubectl config set-context gke_rm-ras-sandbox_europe-west2_k8s-ras --namespace khanm1
kubectl config use-context gke_rm-ras-sandbox_europe-west2_k8s-ras
}

function khanm1dev() {
gcloud container clusters get-credentials k8s-ras --region europe-west2 --project ras-rm-dev
kubectl config set-context gke_ras-rm-dev_europe-west2_k8s-ras --namespace khanm1
kubectl config use-context gke_ras-rm-dev_europe-west2_k8s-ras
}
